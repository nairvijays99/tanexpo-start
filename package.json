{
  "name": "tanexpo-foodrecipe",
  "version": "1.0.0",
  "description": "Shared cross-platform UI components for React Native and TanStack Start, with automatic native/web resolution.",
  "homepage": "https://github.com/nairvijays99/tanexpo-foodrecipe",
  "bugs": {
    "url": "https://github.com/nandorojo/nairvijays99/tanexpo/issues"
  },
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "check-versions": "node ./scripts/check-versions.js",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "format:check": "biome format .",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:i": "pnpm clean && pnpm install",
    "dev": "pnpm --filter ./apps/web run dev",
    "start": "pnpm --filter ./apps/native run start",
    "ios": "pnpm --filter ./apps/native run ios",
    "android": "pnpm --filter ./apps/native run android",
    "web:build": "pnpm --filter ./apps/web run build",
    "web:serve": "pnpm --filter ./apps/web run serve"
  },
  "keywords": [],
  "author": "Vijay S Nair",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "husky": "9.1.7",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-native": "0.81.5",
      "react-native-web": "~0.21.0",
      "@types/react": "19.1.0",
      "@types/react-dom": "19.1.0"
    }
  }
}
